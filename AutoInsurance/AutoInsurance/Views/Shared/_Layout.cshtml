<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Top Notch Auto Insurance</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body><!----style="background-image:url('../../Images/cars.jpg');"-->
    <div class="navbar navbar-expand-lg navbar-light fixed-top bg-light">
        <div class="container">
            <ul class="navbar-nav mr-auto">
                <!---<li class="navbar-brand px-2 py-0">Top Notch Auto Insurance</li>-->
                <li class="nav-item px-2 py-0">  @Html.ActionLink("Home", "Index", "Home") </li>
                <li class="nav-item px-2 py-0">  @Html.ActionLink("Quote", "Quote", "Home") </li>
                <li class="nav-item px-2 py-0">  @Html.ActionLink("Admin", "Admin", "Home") </li>
            </ul>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Top Notch Auto Insurance</p>
        </footer>
    </div>


    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
